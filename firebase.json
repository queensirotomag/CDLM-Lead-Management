
{
  "hosting": {
    "public": ".",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "headers": [
      { "source": "**/*.css", "headers": [ { "key": "Cache-Control", "value": "public, max-age=31536000" } ] },
      { "source": "**/*.js",  "headers": [ { "key": "Cache-Control", "value": "public, max-age=31536000" } ] }
    ]
  }
}
